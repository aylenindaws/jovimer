<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_move_data_tree_inherit" model="ir.ui.view">
        <field name="name">stock.move.data.tree.inherit</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date']" position='after'>
                <field name="supplier_id"/>
                <field name="stock_check" widget="boolean_toggle"/>
            </xpath>
        </field>
    </record>
    <record id="stock_move_data_form_inherit" model="ir.ui.view">
        <field name="name">stock.move.data.form.inherit</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date']" position='after'>
                <field name="paleteur" invisible="True"/>
                <field name="paletgr" invisible="True"/>
                <field name="totalbultos" invisible="True"/>
            </xpath>
        </field>
    </record>
    <record id="stock_view_picking_form_data_form_inherit" model="ir.ui.view">
        <field name="name">stock.view.picking.form.data.form.inherit</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='picking_type_id']" position='after'>
                <field name="analytic_account_id" readonly="1" force_save="1"/>
                <field name="paleteur" invisible="True"/>
                <field name="paletgr" invisible="True"/>
                <field name="totalbultos" invisible="True"/>
            </xpath>
            <xpath expr="//page[@name='operations']//tree//field[@name='date']" position='after'>
                <field name="supplier_id"/>
                <field name="stock_check" widget="boolean_toggle"/>
            </xpath>
        </field>
    </record>
    <record id="stock_view_picking_form_data_tree_inherit" model="ir.ui.view">
        <field name="name">stock.view.picking.tree.data.form.inherit</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='state']" position='after'>
                <field name="analytic_account_id" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>
  