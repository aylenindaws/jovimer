<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_move_data_form_inherit" model="ir.ui.view">
        <field name="name">stock.move.data.form.inherit</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='description_picking']" position='after'>
                <field name="supplier_id"/>
                <field name="stock_check" widget="boolean_toggle"/>
            </xpath>
        </field>
    </record>
    <record id="view_picking_move_tree_data_form_inherit" model="ir.ui.view">
        <field name="name">view.picking.move.tree.data.form.inherit</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_picking_move_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='description_picking']" position='after'>
                <field name="supplier_id"/>
                <field name="stock_check" widget="boolean_toggle"/>
            </xpath>
        </field>
    </record>
</odoo>
  