<odoo>
    <data>
        <!-- start:views -->
        <record model="ir.ui.view" id="jovimer_etiquetas_tree_view">
            <field name="name">jovimer_etiquetas_tree</field>
            <field name="model">jovimer.etiquetas</field>
            <field name="arch" type="xml">
                <tree string="Etiquetas">
                    <field name="name" />
                    <field name="content" />
                    <field name="content_uom" />
                    <field name="content_html" />
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="jovimer_etiquetas_form_view">
            <field name="name">jovimer_etiquetas_form</field>
            <field name="model">jovimer.etiquetas</field>
            <field name="arch" type="xml">
                <form string="Etiquetas">
                    <sheet>
                        <group>
                            <field name="name" />
                            <field name="content" />
                            <field name="content_uom" />
                            <field name="content_html" />
                            <field name="order_line_id" />
                            <field name="order_id" />
                            <field name="purchase_line_id" />
                            <field name="purchase_id" />
                            <field name="dossier_id" />
                            <field name="dossier_campaign" />
                            <field name="dossier_series_id" />
                            <field name="dossier_num" />
                            <field name="label_template_id" />
                            <field name="partner_id" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- end:views -->

        <!-- start:actions -->
        <record model="ir.actions.act_window"
            id="jovimer_etiquetas_list_action">
            <field name="name">jovimer_etiquetas</field>
            <field name="res_model">jovimer.etiquetas</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Create the first jovimer etiquetas
                </p>
            </field>
        </record>
    </data>
</odoo>