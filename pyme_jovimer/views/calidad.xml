<odoo>
    <data>



<record model="ir.ui.view" id="jovimer_calidad_valoracionesv_view_search">
      <field name="name">JOVIMER - Buscador Lineas Venta </field>
      <field name="model">account.invoice.line</field>
      <field name="priority">0</field>
      <field name="arch" type="xml">
      <search string="Search Invoice">
	  
	  
					
					 <field name="expediente" string="Expediente" context="{'active_test': False}" filter_domain="[('expediente','=',self)]"/>
                   
                    <field name="partner_id" operator="child_of" string="Cliente"/>
					<field name="invoice_id" operator="child_of" string="Factura"/>
					<field name="refcliente" string="Referencia Cliente"/>
					<field name="fechafact"  operator="child_of"/>
					<field name="productomaestro"/>
					<field name="plantilla"/>
					<separator/>
					<filter name="plantilabio" string="Productos BIO" domain="[('plantilla','ilike', 'BIO'),('productomaestro','!=', False),('nobio','!=', True),('lineacompra','!=', False),('fechafact','!=', False),('nobioexp','=', False),('nobioexp','=', False),('lineacompracalidadpartner.nobio','=', False)]"/>
                    <group expand="0" string="Group By">
                        <filter name="group_by_partner_id" string="Cliente" context="{'group_by':'partner_id'}"/>
						<filter name="pais" string="Pais" context="{'group_by':'paislins'}"/>
						<filter name="productomaestro" string="Producto Mestro" context="{'group_by':'productomaestro'}"/>
                        <filter string="Expediente" name="expegr" context="{'group_by':'expediente'}"/>
                    </group>
               </search>

      </field>
    </record>




<record model="ir.ui.view" id="jovimer_calidad_valoracionesc_view_search">
      <field name="name">JOVIMER - Buscador Lineas Compra</field>
      <field name="model">account.invoice.line</field>
      <field name="priority">0</field>
      <field name="arch" type="xml">
      <search string="Search Invoice">
	  
	  <field name="expediente" string="Expediente" context="{'active_test': False}" filter_domain="[('expediente','=',self)]"/>
                    <field name="partner_id" operator="child_of" string="Cliente"/>
                    <field name="fechapedidosalida" string="Fecha Pedido/Salida"/>
					
					<field name="invoice_id" operator="child_of" string="Factura"/>
					<field name="refcliente" string="Referencia Cliente"/>
					
					<field name="fechafact"  operator="child_of"/>
					<field name="productomaestro"/>
					<field name="plantilla"/>
                    <separator/>
					<filter name="plantilabio" string="Productos BIO" domain="[('plantilla','ilike', 'BIO'),('productomaestro','!=', False),('nobio','!=', True),('fechafact','!=', False),('nobioexp','=', False)]"/>
					
                    <group expand="0" string="Group By">
                        <filter name="group_by_partner_id" string="Cliente" context="{'group_by':'partner_id'}"/>
						<filter name="pais" string="Pais" context="{'group_by':'paislins'}"/>
						<filter name="productomaestro" string="Producto Mestro" context="{'group_by':'productomaestro'}"/>
                        <filter string="Expediente" name="expegr" context="{'group_by':'expediente'}"/>
						<filter string="Fecha Pedido / Salida" name="fechapedidosalidagr" context="{'group_by':'fechapedidosalida'}"/>
                    </group>
               </search>

      </field>
    </record>


 <record model="ir.ui.view" id="jovimer_calidad_albaval_prov_view_tree">
      <field name="name">Pyme - PROVEEDORES </field>
      <field name="model">account.invoice.line</field>
	  <field name="priority">0</field>
      <field name="arch" type="xml">
   <tree string="Bill lines" editable="top">
                                    <field name="sequence" widget="handle" invisible="1"/>
                                    <field name="expediente" readonly="1" string="Exp"/>
                                    <field name="fechapedidosalida" readonly="1"/>
									<field name="fechafact"/>
									<field name="nobioexp" invisible="1"/>  
									<field name="partner_id" readonly="1"/>
									<field name="paislins" readonly="1"/>
									<field name="invoice_id" readonly="1" string="Documento"/>
									<field name="refcliente" readonly="1" string="Ref Proveedor"/>
                                    <field name="lineacompra" readonly="1" invisible="1"/>
									<field name="lineaventa" readonly="1" invisible="1"/>
									<field name="product_id"  context="{'partner_id': parent.partner_id}" domain="[('purchase_ok','=',True)]"/>
                                    <field name="name" invisible="1"/>
									<field name="variedad" readonly="1"/>
                                    <field name="confeccion"/>
									<field name="marca" readonly="1"/>
									<field name="envase" readonly="1"/>
									<field name="categoria" readonly="1"/>
                                    <field name="cantidadpedido" widget="integer" />
									<field name="unidadpedido"/>
									<field name="bultos" widget="integer"  string="BxP"/>	
									<field name="totalbultos" string="Tot Blt"/>									
									<field name="totalkglin"/>	
									<field name="pricej" invisible="1" readonly="1" string="PVD Comp"/>
									<field name="cantidadj" invisible="1" readonly="1"/>
									<field name="unidabultoj" invisible="1" readonly="1" string="UD Comp"/>
									<field name="subtotalj" readonly="1" invisible="1"/>
                                  
									
									<field name="company_id" invisible="1"/>
                                    <field name="account_id" groups="account.group_account_user" domain="[('company_id', '=', parent.company_id), ('internal_type', '=', 'other'), ('deprecated', '=', False)]"/>
                                    <field name="account_analytic_id" groups="analytic.group_analytic_accounting" domain="[('company_id', '=', parent.company_id)]" context="{'default_partner_id': parent.partner_id}"/>
                                    <field name="analytic_tag_ids" groups="analytic.group_analytic_accounting" widget="many2many_tags" options="{'color_field': 'color'}"/>
									<field name="price_unit" string="PVD-Alb"/>
									<field name="quantity"/>
									<field name="unidabulto" string="UD-Alb"/>
									
									
                                    <field name="uom_id" groups="product.group_uom"/>
                                    
                                    <field name="discount" string="Dto" invisible="1"/>
                                    <field name="invoice_line_tax_ids"  invisible="1" widget="many2many_tags" options="{'no_create': True}" context="{'type':parent.type}" domain="[('type_tax_use','=','purchase'),('company_id', '=', parent.company_id)]"/>
                                    <field name="price_subtotal" string="Subtotal" invisible="1"/>
                                    <field name="currency_id" invisible="1"/>
                                </tree>
           </field>
    </record>  



 <record model="ir.ui.view" id="jovimer_calidad_albaval_cli_view_tree">
      <field name="name">Pyme - CLIENTES </field>
      <field name="model">account.invoice.line</field>
	  <field name="priority">0</field>
      <field name="arch" type="xml">
   <tree string="Bill lines" editable="top">
                                    <field name="sequence" widget="handle" invisible="1"/>
									<field name="id" invisible="1"/>
									<field name="fechapedidosalida" readonly="1"/>
                                    <field name="expediente" readonly="1"/>
									<field name="fechafact" readonly="1"/>
									<field name="partner_id" readonly="1"/>
									<field name="paislins" readonly="1"/>
									<field name="invoice_id" readonly="1" string="Documento"/>
                                    <field name="lineacompra" readonly="1"/>
									<field name="lineacompracalidadpartner" readonly="1"/>
									
									<field name="lineaventa" invisible="1" readonly="1"/>
									<field name="product_id"  readonly="1"/>
                                    <field name="name" invisible="1"/>
									<field name="variedad" readonly="1"/>
                                    <field name="confeccion" options="{'no_create': True}"/>
									<field name="marca" readonly="1"/>
									<field name="envase" readonly="1"/>
									<field name="categoria" readonly="1"/>
                                    <field name="cantidadpedido" widget="integer" readonly="1"/>
									<field name="unidadpedido" readonly="1"/>
									<field name="bultos" string="BxP" widget="integer" readonly="1"/>
									<field name="totalbultos" string="Bultos" widget="integer" readonly="1"/>	
									<field name="totalkglin" readonly="1"/>									
								
									<field name="company_id" invisible="1"/>
                                    <field name="account_id" groups="account.group_account_user" domain="[('company_id', '=', parent.company_id), ('internal_type', '=', 'other'), ('deprecated', '=', False)]"/>
                                    <field name="account_analytic_id" groups="analytic.group_analytic_accounting" domain="[('company_id', '=', parent.company_id)]" context="{'default_partner_id': parent.partner_id}"/>
                                    <field name="analytic_tag_ids" groups="analytic.group_analytic_accounting" widget="many2many_tags" options="{'color_field': 'color'}"/>
									<field name="price_unit" string="PVD" readonly="1"/>
									<field name="quantity" readonly="1"/>
									<field name="unidabulto" string="UD Alb" readonly="1"/>
									
									
                                    <field name="uom_id" groups="product.group_uom" invisible="1" />
                                    
                                    <field name="discount" invisible="1" />
                                    <field name="invoice_line_tax_ids"  invisible="1" widget="many2many_tags" options="{'no_create': True}" context="{'type':parent.type}" domain="[('type_tax_use','=','purchase'),('company_id', '=', parent.company_id)]"/>
                                    <field name="price_subtotal" string="Subtotal"/>
                                    <field name="currency_id" invisible="1"/>
                                </tree>
           </field>
    </record>  




	<!--	LIstado de Valoraciones -->
	<!--	LIstado de Valoraciones -->
	<!--	LIstado de Valoraciones -->


        <record id="action_jovimer_calidad_facturas_clienteval" model="ir.actions.act_window">
            <field name="name">Jovimer - Facturas Cliente</field>
            <field name="res_model">account.invoice.line</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field eval="False" name="view_id"/>
            <field name="domain">[('invoice_id.type','=','out_invoice'),('invoice_id.journal_id','in', [11,13,14])]</field>
            <field name="context">{}</field>
            <field name="search_view_id" ref="jovimer_calidad_valoracionesv_view_search"/> 
			<field name="view_ids" eval="[(5, 0, 0),
                            (0, 0, {'view_mode': 'tree', 'view_id': ref('jovimer_calidad_albaval_cli_view_tree')})]"/>

            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Facturas Cliente
              </p>
            </field>
        </record>


        <record id="action_jovimer_calidad_albaranes_clienteval" model="ir.actions.act_window">
            <field name="name">Jovimer - Salidas  / Albaranes Cliente</field>
            <field name="res_model">account.invoice.line</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field eval="False" name="view_id"/>
            <field name="domain">[('invoice_id.type','=','out_invoice'),('invoice_id.journal_id','=', 10)]</field>
            <field name="context">{}</field>
            <field name="search_view_id" ref="jovimer_calidad_valoracionesv_view_search"/> 
			<field name="view_ids" eval="[(5, 0, 0),
                            (0, 0, {'view_mode': 'tree', 'view_id': ref('jovimer_calidad_albaval_cli_view_tree')})]"/>

            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Salidas  / Albaranes Cliente
              </p>
            </field>
        </record>


        <record id="action_jovimer_calidad_albaranes_proveedorval" model="ir.actions.act_window">
            <field name="name">Jovimer - Entradas  / Albaranes proveedor</field>
            <field name="res_model">account.invoice.line</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field eval="False" name="view_id"/>
            <field name="domain">[('invoice_id.type','=','in_invoice'),('invoice_id.journal_id','=', 9)]</field>
            <field name="context">{}</field>
            <field name="search_view_id" ref="jovimer_calidad_valoracionesc_view_search"/> 
			<field name="view_ids" eval="[(5, 0, 0),
                            (0, 0, {'view_mode': 'tree', 'view_id': ref('jovimer_calidad_albaval_prov_view_tree')})]"/>

            <field name="help" type="html">
              <p class="o_view_nocontent_smiling_face">
                Salidas  / Albaranes proveedor
              </p>
            </field>
        </record>


			  
         <menuitem id="menu_jovimercalidad" 
              name="Calidad" 
              web_icon="pyme_menus,static/menu/tesoreria.png"
              sequence="10"/>

  

          <menuitem id="menu_entradassalidas" 
              name="Entradas y Salidas" 
			  parent="menu_jovimercalidad"
              sequence="2"/>
	
		 <menuitem name="Entradas / Albaranes Proveedor"
              id="menu_jovimer_valoraciones_comp"
              parent="menu_entradassalidas"
