          <menuitem id="menu_lineasfacturas" 
              name="Lineas Facturas" 
			  parent="menu_jovimercalidad"
              sequence="2"/>
			  
		 <menuitem name="Lineas Facturas Cliente"
              id="menu_jovimer_factcli_vta"
